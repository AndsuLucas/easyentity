{
    "name": "andsu/easyentity",
    "description": "My personal package for create entities",
    "type": "library",
    "require-dev": {
        "phpstan/phpstan": "^1.1",
        "phpmd/phpmd": "^2.10",
        "squizlabs/php_codesniffer": "^3.7",
        "phpunit/phpunit": "^10.5",
        "phpro/grumphp": "*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Andsu\\Easyentity\\": "app/",
            "Andsu\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "grumphp": "grumphp run"
    },
    "authors": [
        {
            "name": "Anderson Lucas Silva de Oliveira"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpro/grumphp-shim": true,
            "phpro/grumphp": true
        }
    },
    "require": {
        "nikic/php-parser": "^5.0"
    }
}
